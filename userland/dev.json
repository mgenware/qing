{
  "extends": "base.json",
  "http": {
    "port": 8000,
    "static": {
      "url": "/static",
      "dir": "./static"
    },
    "log_404_error": true
  },
  "templates": {
    "dir": "./templates"
  },
  "logging": {
    "dir": "../appdata/log/dev"
  },
  "db": {
    "conn_string": "qing_dev:qing_dev_pwd@tcp(db:3306)/qing_dev_db?parseTime=true&collation=utf8mb4_unicode_ci&clientFoundRows=true"
  },
  "res_server": {
    "url": "/res",
    "dir": "../appdata/res_server"
  },
  "debug": {
    "quick_login": true,
    "reload_views_on_refresh": true,
    "panic_on_unexpected_html_errors": false,
    "panic_on_unexpected_json_errors": false,
    "turbo_web": {
      "dir": "../web",
      "url": "/static-turbo"
    }
  },
  "app_profile": {
    "dir": "../appdata/app_profile.json"
  }
}
