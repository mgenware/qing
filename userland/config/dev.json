{
  "extends": "base.json",
  "http": {
    "port": 8000,
    "static": {
      "url": "/static",
      "dir": "./static"
    },
    "log_404_error": true
  },
  "templates": {
    "dir": "./templates"
  },
  "logging": {
    "dir": "/qing_data/log/dev"
  },
  "db": {
    "user": "qing_dev",
    "pwd": "qing_dev_pwd",
    "host": "db",
    "port": 3306,
    "database": "qing_dev_db",
    "params": "?parseTime=true&collation=utf8mb4_unicode_ci&clientFoundRows=true"
  },
  "res_server": {
    "url": "/res",
    "dir": "/qing_data/res_server"
  },
  "debug": {
    "reload_views_on_refresh": true,
    "turbo_web": {
      "dir": "/qing/web",
      "url": "/static-turbo"
    }
  },
  "app_profile": {
    "dir": "/qing_data/app_profile.json"
  },
  "app_settings": {
    "file": "/qing_data/app_settings.json"
  }
}
