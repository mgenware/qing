    "construct-style-sheets-polyfill": "^2.3.5",
    "date-fns": "^2.10.0",
    "escape-html": "^1.0.3",
    "fx214": "^1.1.0",
    "js-cookie": "^2.2.1",
    "kangxi-editor": "^0.6.0",
    "lit-button": "^3.5.0",
    "lit-element": "^2.2.1",
    "lit-props": "^1.0.0",
    "page": "^1.11.5",
    "qing-dialog-component": "^0.0.9"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-json": "^4.0.2",
