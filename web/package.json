{
  "name": "qing-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "yarn run lint && yarn run qing:prepare && cross-env NODE_ENV=production rollup -c",
    "dev": "yarn run qing:prepare && cross-env NODE_ENV=development rollup -c -w",
    "lint": "eslint --ext .ts src/ && lit-analyzer \"src/**/*.ts\"",
    "qing:prepare": "yarn --cwd ../tools build && del-cli static/d/js"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@github/image-crop-element": "^3.0.8",
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "bowhead-js": "^3.0.0",
    "construct-style-sheets-polyfill": "^2.3.5",
    "date-fns": "^2.13.0",
    "escape-html": "^1.0.3",
    "fx214": "^1.1.0",
    "js-cookie": "^2.2.1",
    "kangxi-editor": "^0.6.0",
    "lit-button": "^3.5.0",
    "lit-element": "^2.2.1",
    "lit-props": "^1.0.0",
    "page": "^1.11.6",
    "qing-dialog-component": "^0.0.9"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@types/escape-html": "^0.0.20",
    "@types/js-cookie": "^2.2.6",
    "@types/page": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "cross-env": "^7.0.1",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "lit-analyzer": "^1.1.10",
    "rollup": "^2.8.2",
    "rollup-plugin-lit-css-ex": "^2.0.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "typescript": "^3.8.3"
  }
}
