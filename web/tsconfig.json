{
  "extends": "mgenware-tsconfig-browser-x",
  "include": ["./src/**/*"],
  "compilerOptions": {
    "noEmit": true,
    "importHelpers": false,
    "useDefineForClassFields": false,
    "rootDir": "./src",
    "baseUrl": "./src",
    "typeRoots": ["./src/@types/**/*", "node_modules/@types"],
    "declaration": false,
    "incremental": true,
    "sourceMap": true,
    "preserveWatchOutput": true
  }
}
